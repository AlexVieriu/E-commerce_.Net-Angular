{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C#: API Debug",
      "type": "dotnet",
      "request": "launch",
      "projectPath": "${workspaceFolder}\\skinet\\API\\API.csproj"
    },
    {
      "name": "Angular",
      "request": "launch",
      "type": "chrome",
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}\\skinet\\client",
      "preLaunchTask": "npm: start",
      "skipFiles": [
        "chrome-error:\u002F\u002F*"
      ],
      "timeout": 30000,
      "serverReadyAction": {
        "pattern": "Compiled successfully",
        "uriFormat": "http://localhost:4200",
        "action": "openExternally"
      }
    },
  ],
}